{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "ie > 8"],
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-assign",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ]
}
